{
  "name": "test-ui-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p start:*",
    "start:api": "prism mock -d -p 2000 ./public/api-docs.yaml",
    "start:app": "react-scripts start",
    "storybook": "start-storybook -p 4000 -s public --ci",
    "prebuild": "npm run shield:build",
    "build": "run-s build:*",
    "build:app": "react-scripts build",
    "build:storybook": "build-storybook -s public -o build/storybook",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "heroku-postbuild": "env-cmd -f .env.heroku npm run build",
    "prettier": "prettier \"src/**/*.{html,css,js}\"",
    "format": "npm run prettier -- --write",
    "lint": "eslint src stories/**/*.mdx",
    "lint:fix": "npm run lint -- --fix",
    "validate": "npm run prettier -- --list-different && npm run lint",
    "postinstall": "npm run shield:install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json,css,md} stories/**/*.mdx": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:mdx/recommended"
    ],
    "rules": {
      "react/no-unknown-property": [
        2
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.3.1",
    "@testing-library/dom": "^6.12.2",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.1.0",
    "@tippy.js/react": "^3.1.1",
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-datepicker": "^2.11.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "react-placeholder": "^3.0.2",
    "react-responsive": "^8.0.3",
    "react-scripts": "^3.3.1",
    "sanitize.css": "^11.0.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
   
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "colorette": "^1.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mdx": "^1.6.8",
    "eslint-plugin-react": "^7.18.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gulp": "^4.0.2",
    "gulp-theo": "^2.0.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "playwright": "^0.10.0",
    "postcss-gap-properties": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^4.0.1",
    "postcss-reporter": "^6.0.1",
    "postcss-scss": "^2.0.0",
    "postcss-url": "^8.0.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-cjs-es": "^0.9.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.6",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-terser": "^5.2.0",
  }
}